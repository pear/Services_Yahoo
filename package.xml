<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE package SYSTEM "http://pear.php.net/dtd/package-1.0">
<package version="1.0">
  <name>Services_Yahoo</name>
  <summary>Provides access to the Yahoo! Web Services</summary>
  <description>
    Services_Yahoo provides object-oriented interfaces to the web
    service capabilities of Yahoo.
  </description>
  <maintainers>
    <maintainer>
      <user>mj</user>
      <name>Martin Jansen</name>
      <email>mj@php.net</email>
      <role>lead</role>
    </maintainer>
  </maintainers>
  <release>
    <version>0.1.0</version>
    <date>2005-xx-xx</date>
    <license>PHP License</license>
    <state>alpha</state>
    <notes>Initial release</notes>
    <deps>
      <dep type="pkg" rel="has" optional="no">HTTP_Request</dep>
      <dep type="pkg" rel="ge" version="1.3.3" optional="no">PEAR</dep>
      <dep type="php" rel="ge" version="5.0.0" optional="no" />
      <dep type="ext" rel="has" optional="no">simplexml</dep>
    </deps>
    <filelist>
      <dir name="/" baseinstalldir="Services">
        <dir name="Yahoo">
          <file role="php" name="Exception.php" />
          <file role="php" name="Search.php" />
          <dir name="Search" role="php">
            <file name="AbstractSearch.php" />
            <file name="image.php" />
            <file name="local.php" />
            <file name="news.php" />
            <file name="Response.php" />
            <file name="video.php" />
            <file name="web.php" />
          </dir>
        </dir>
      </dir>
    </filelist>
  </release>
</package>
